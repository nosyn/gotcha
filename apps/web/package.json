{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000 --clearScreen false",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "codegen:compile": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen -w --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.8",
    "@emotion/react": "^11.10.6",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mantine/core": "^6.0.0",
    "@mantine/ds": "^6.0.0",
    "@mantine/form": "^6.0.0",
    "@mantine/hooks": "^6.0.0",
    "@mantine/notifications": "^6.0.0",
    "@tabler/icons-react": "^2.7.0",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.3",
    "ky": "^0.33.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "zod": "^3.20.6",
    "zustand": "^4.3.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}